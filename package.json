{
  "version": "1.0.1",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@rodjosh/react-responsive-props",
  "author": "rodjosh",
  "module": "dist/react-responsive-props.esm.js",
  "size-limit": [
    {
      "path": "dist/react-responsive-props.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-responsive-props.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "husky": "^7.0.4",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "react-responsive": "^9.0.0-beta.6"
  },
  "publishConfig": {
    "access": "public"
  },
  "description": "Is a simple library on top of react-responsive that allows to change component props based on breakpoints",
  "directories": {
    "example": "example"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rodjosh/react-responsive-props.git"
  },
  "keywords": [
    "react",
    "responsive"
  ],
  "bugs": {
    "url": "https://github.com/rodjosh/react-responsive-props/issues"
  },
  "homepage": "https://github.com/rodjosh/react-responsive-props#readme"
}
